            </properties>
        </profile>
        <profile>
            <id>test</id>
            <properties>
                <custom-deploy-css>WebContent/css/custom-deploy.css</custom-deploy-css>
                <context>WebContent/META-INF/context.xml.7</context>
                <db-context>WebContent/WEB-INF/db-context.xml</db-context>
                <mvc-context>WebContent/WEB-INF/mvc-context.xml.test</mvc-context>
                <spring-servlet>WebContent/WEB-INF/spring-servlet.xml.test</spring-servlet>
                <web-xml>WebContent/WEB-INF/web.xml.test</web-xml>
                <auac-properties>WebContent/WEB-INF/classes/auac.test.properties</auac-properties>
            </properties>
        </profile>
    </profiles>

    <build>
        <sourceDirectory>src</sourceDirectory>
        <testSourceDirectory>test</testSourceDirectory>
        <resources>
            <resource>
                <directory>src</directory>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
            </resource>
        </resources>
        <plugins>
            <!-- compiler -->
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.0</version>
                <configuration>
                    <compilerVersion>${jdk.version}</compilerVersion>
                </configuration>
            </plugin>
            <!-- copy profile's files with original names -->
            <plugin>
                <groupId>com.coderplus.maven.plugins</groupId>
                <artifactId>copy-rename-maven-plugin</artifactId>
                <version>1.0.1</version>
                <executions>
                    <execution>
                        <id>rename-file</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <fileSets>
                                <fileSet>
                                    <sourceFile>${custom-deploy-css}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/css/custom-deploy.css</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${context}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/META-INF/context.xml</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${db-context}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/WEB-INF/db-context.xml</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${mvc-context}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/WEB-INF/mvc-context.xml</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${spring-servlet}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/WEB-INF/spring-servlet.xml</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${web-xml}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/WEB-INF/web.xml</destinationFile>
                                </fileSet>
                                <fileSet>
                                    <sourceFile>${auac-properties}</sourceFile>
                                    <destinationFile>${profile-generated-files-dir}/WEB-INF/classes/auac.properties</destinationFile>
                                </fileSet>
                            </fileSets>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- definition of war based on profiles -->
            <plugin>
                <artifactId>maven-war-plugin</artifactId>
                <version>3.2.2</version>
                <configuration>
                    <failOnMissingWebXml>false</failOnMissingWebXml>
                    <webResources>
                        <webResource>
                            <directory>WebContent</directory>
                            <includes>
                                <include>**/*</include>
                            </includes>
                            <excludes>
                                <exclude>css/custom-deploy*</exclude>
                                <exclude>WEB-INF/db-context*</exclude>
                                <exclude>WEB-INF/mvc-context*</exclude>
                                <exclude>WEB-INF/spring-servlet*</exclude>
                                <exclude>WEB-INF/web.*</exclude>
                                <exclude>WEB-INF/web cas*</exclude>
                                <exclude>WEB-INF/classes/auac*</exclude>
                                <exclude>META-INF/context*</exclude>
                            </excludes>
                        </webResource>
                        <webResource>
                            <directory>${profile-generated-files-dir}</directory>
                            <includes>
                                <include>**/*</include>
                            </includes>
                        </webResource>
                    </webResources>
                </configuration>
            </plugin>
            <!-- sources -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>3.0.1</version>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>


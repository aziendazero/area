    <profiles>
        <profile>
            <id>tomcat7</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <custom-deploy-css>WebContent/css/custom-deploy.css</custom-deploy-css>
                <context>WebContent/META-INF/context.xml.7</context>
                <db-context>WebContent/WEB-INF/db-context.xml</db-context>
                <mvc-context>WebContent/WEB-INF/mvc-context.xml</mvc-context>
                <spring-servlet>WebContent/WEB-INF/spring-servlet.xml</spring-servlet>
                <web-xml>WebContent/WEB-INF/web.xml</web-xml>
                <auac-properties>WebContent/WEB-INF/classes/auac.properties</auac-properties>
            </properties>
        </profile>
        <profile>
            <id>tomcat8</id>
            <properties>
                <custom-deploy-css>WebContent/css/custom-deploy.css</custom-deploy-css>
                <context>WebContent/META-INF/context.xml.8</context>
                <db-context>WebContent/WEB-INF/db-context.xml</db-context>
                <mvc-context>WebContent/WEB-INF/mvc-context.xml</mvc-context>
